{
  "name": "@tylertech/tyler-icons",
  "version": "0.2.0",
  "description": "The tree-shakable Tyler SVG icon library.",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "svg-to-ts && npm run copy-assets",
    "copy-assets": "cpx package.json dist && cpx README.md dist",
    "changelog": "standard-changelog -i CHANGELOG.md -k package.json",
    "serve": "browser-sync start --server --files '*.html,*.css,*.ts,*.js' --start-path /demo"
  },
  "keywords": [
    "tyler",
    "icons",
    "svg"
  ],
  "author": "Kieran Nichols <kieran.nichols@tylertech.com",
  "license": "ISC",
  "devDependencies": {
    "@tylertech/tyler-components-web": "^1.4.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "browser-sync": "^2.26.13",
    "cpx": "^1.5.0",
    "http-server": "^0.12.3",
    "standard-changelog": "^2.0.27",
    "svg-to-ts": "^5.7.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
